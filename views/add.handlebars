          <a href="#">{{name}}</a>
          <p>{{description}}</p>
          <img src={{imageURL}} class="img-responsive", width = 300px>
        </div>
    {{/each}}

</div>

	<p><a href="\">Back &raquo;</a><p>
	
	<script src="/js/jquery-1.11.0.js"></script>
	<script src="/js/bootstrap.js"></script>	    
	<script src="js/hci-friends.js"></script>
</body>
</html>
